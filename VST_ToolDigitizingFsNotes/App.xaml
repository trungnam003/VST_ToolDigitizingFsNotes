<Application x:Class="VST_ToolDigitizingFsNotes.AppMain.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:VST_ToolDigitizingFsNotes.AppMain.ViewModels"
             xmlns:view="clr-namespace:VST_ToolDigitizingFsNotes.AppMain.Views"
             Startup="Application_Startup"
             Exit="Application_Exit">
    <Application.Resources>
        <DataTemplate DataType="{x:Type TypeName=vm:HomeViewModel}">
            <view:HomeView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type TypeName=vm:SettingViewModel}">
            <view:SettingView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type TypeName=vm:TestMapDataViewModel}">
            <view:TestMapDataView/>
        </DataTemplate>
    </Application.Resources>
</Application>
